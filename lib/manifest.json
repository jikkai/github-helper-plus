{
  "name": "Github Helper Plus",
  "version": "0.0.1",
  "minimum_chrome_version": "37",
  "manifest_version": 2,
  "description": "Get a better idea about github!",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  },
  "permissions": [
    "*://*.github.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["app.js"],
      "css": ["app.css"]
    }
  ]
}
