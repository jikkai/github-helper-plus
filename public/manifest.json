{
  "name": "Github Helper Plus",
  "version": "0.1.1",
  "minimum_chrome_version": "37",
  "manifest_version": 2,
  "description": "Get a better idea about github!",
  "icons": {
    "128": "icons/icon_128.png",
    "48": "icons/icon_48.png",
    "16": "icons/icon_16.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "*://*.github.com/*"
  ],
  "background": {
    "scripts": ["background/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "css": ["app/app.css"]
    }
  ]
}
